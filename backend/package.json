{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "babel ./src -d dist --extensions '.ts'",
    "start": "if [ $APP_ENV = \"production\" ]; then npm run start:prod; else npm run start:dev; fi",
    "start:prod": "export NODE_ENV=production && npm run build && node dist/index.js",
    "start:dev": "export NODE_ENV=development && nodemon index.dev.js"
  },
  "author": "Jatin Agrawal",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.14",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "winston": "^3.3.3"
  }
}
